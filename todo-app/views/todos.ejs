<table>
    <% for (var i=0;i < allTodos.length ;i++) { %>
        <tr>
            <td>
                <%= allTodos[i].title %>
                <%= allTodos[i].dueDate %>
                <%= allTodos[i].completed ? "[x]" : "[]" %>
            </td>
        </tr>
        <% }
    %>
    <tr></tr>
</table>