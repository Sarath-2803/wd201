<div class="py-10 col-start-3">
    <h5 class="font-bold">Overdue</h5>
    <table>
    <!-- <ul class="list-none"> -->
        <% var countOverdue = 0; %>
        <% for (var i = 0; i < allTodos.length; i++) { %>
            <% if (allTodos[i].dueDate < new Date().toISOString().split('T')[0]) { %>
                <% countOverdue++; %>
                <!-- <li class="todo-item"> -->
                    <tr>
                        <td>
                    <div class="flex w-fit my-2 px-2 py-1 items-center">
                        <input type="checkbox" class="mr-2" id="todoCheckbox<%= allTodos[i].id %>" name="todoCheckbox<%= allTodos[i].id %>" <%= allTodos[i].completed ? "checked" : "" %> value="<%= allTodos[i].id %>" onclick="updateTodo('<%= allTodos[i].id %>')">
                        <label class="text-black" for="todoCheckbox<%= allTodos[i].id %>"><%= allTodos[i].title %></label>
                        <a href="#" class="hidden ml-2 trash-icon" onclick="deleteTodo('<%= allTodos[i].id %>')">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4">
                                <path stroke-linecap="round" stroke-linejoin="round" d="m14.74 9-.346 9m-4.788 0L9.26 9m9.968-3.21c.342.052.682.107 1.022.166m-1.022-.165L18.16 19.673a2.25 2.25 0 0 1-2.244 2.077H8.084a2.25 2.25 0 0 1-2.244-2.077L4.772 5.79m14.456 0a48.108 48.108 0 0 0-3.478-.397m-12 .562c.34-.059.68-.114 1.022-.165m0 0a48.11 48.11 0 0 1 3.478-.397m7.5 0v-.916c0-1.18-.91-2.164-2.09-2.201a51.964 51.964 0 0 0-3.32 0c-1.18.037-2.09 1.022-2.09 2.201v.916m7.5 0a48.667 48.667 0 0 0-7.5 0" />
                            </svg>
                        </a>
                    </div>
                </td>
                </tr>
                <!-- </li> -->
            <% } %>
        <% } %>
        <tr>
            <td>
                <label id="count-overdue" class="py-4 text-blue-500">count-overdue = <%= countOverdue %></label>
            </td>
        </tr>
        
    <!-- </ul> -->
</table>

